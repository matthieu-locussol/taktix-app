{
   "name": "server",
   "version": "1.24.6",
   "scripts": {
      "postinstall": "prisma generate",
      "build": "prisma generate && tsc",
      "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
      "eslint": "eslint src --report-unused-disable-directives",
      "dev": "prisma generate && ts-node-dev --respawn --transpile-only src/index.ts",
      "dev:debug": "DEBUG=colyseus:* ts-node-dev --respawn --transpile-only src/index.ts",
      "start": "node ./dist/index.js",
      "test": "vitest --run",
      "studio": "prisma studio"
   },
   "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com> (https://www.matthieu-locussol.com)",
   "license": "MIT",
   "dependencies": {
      "@colyseus/core": "^0.15.23",
      "@colyseus/monitor": "^0.15.4",
      "@colyseus/playground": "^0.15.11",
      "@colyseus/ws-transport": "^0.15.1",
      "@fastify/cors": "^8.5.0",
      "@fastify/websocket": "^8.3.1",
      "@prisma/client": "^5.8.1",
      "@prisma/extension-accelerate": "^0.6.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "express-basic-auth": "^1.2.1",
      "fastify": "^4.25.2",
      "mobx": "^6.12.0",
      "prisma": "^5.8.1",
      "shared": "workspace:latest",
      "ts-pattern": "^5.0.6",
      "uuid": "^9.0.1",
      "vitest": "^1.2.1",
      "ws": "^8.16.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/express-serve-static-core": "^4.17.41",
      "@types/node": "^20.11.5",
      "@types/uuid": "^9.0.7",
      "@types/ws": "^8.5.10",
      "config-eslint": "workspace:latest",
      "config-typescript": "workspace:latest",
      "eslint": "^8.56.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.3.3"
   }
}
