{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "build": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint", "build-shared", "^build"]
      },
      "build-shared": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint"]
      },
      "clean": {
         "cache": false
      },
      "dev": {
         "cache": false,
         "persistent": true
      },
      "eslint": {},
      "start": {
         "outputs": [],
         "dependsOn": ["test"]
      },
      "test": {
         "outputs": [],
         "dependsOn": ["build"]
      }
   }
}
