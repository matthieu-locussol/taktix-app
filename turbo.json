{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "build": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint", "^build"]
      },
      "clean": {
         "cache": false
      },
      "dev": {
         "cache": false
      },
      "eslint": {},
      "start": {
         "outputs": [],
         "dependsOn": ["^build", "^test"]
      },
      "test": {
         "outputs": [],
         "dependsOn": ["^build"]
      }
   }
}
