{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "build": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint", "^build"]
      },
      "clean": {
         "cache": false
      },
      "dev": {
         "cache": false,
         "persistent": true
      },
      "dev:app": {
         "cache": false,
         "persistent": true
      },
      "dev:client": {
         "cache": false,
         "persistent": true
      },
      "dev:server": {
         "cache": false,
         "persistent": true
      },
      "eslint": {},
      "start": {},
      "test": {}
   }
}
