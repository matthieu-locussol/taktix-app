{
   "$schema": "https://turbo.build/schema.json",
   "pipeline": {
      "build": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint", "^build"]
      },
      "build-shared": {
         "outputs": ["dist/**"],
         "dependsOn": ["eslint-shared"]
      },
      "clean": {
         "cache": false
      },
      "dev": {
         "cache": false,
         "persistent": true
      },
      "eslint": {
         "dependsOn": ["build-shared"]
      },
      "eslint-shared": {},
      "start": {
         "outputs": [],
         "dependsOn": ["test"]
      },
      "test": {
         "outputs": [],
         "dependsOn": ["build"]
      }
   }
}
